osc@pop-os:~$ mkdir IslamHany
osc@pop-os:~$ cd IslamHany/
osc@pop-os:~/IslamHany$ mkdir Documents
osc@pop-os:~/IslamHany$ cd Documents/
osc@pop-os:~/IslamHany/Documents$ touch file1.txt file2.txt file3.txt
osc@pop-os:~/IslamHany/Documents$ ls
file1.txt  file2.txt  file3.txt
osc@pop-os:~/IslamHany/Documents$ cd ..
osc@pop-os:~/IslamHany$ touch ".hidden_file"
osc@pop-os:~/IslamHany$ ls -a
.  ..  Documents  .hidden_file
osc@pop-os:~/IslamHany$ nano ~ Documents/file1.txt
osc@pop-os:~/IslamHany$ cat Documents/file1.txt 
file1.txt 
osc@pop-os:~/IslamHany$ cd Documents/
osc@pop-os:~/IslamHany/Documents$ mkdir "-"
osc@pop-os:~/IslamHany/Documents$ ls
-  file1.txt  file2.txt  file3.txt
osc@pop-os:~/IslamHany/Documents$ cd ./-/
osc@pop-os:~/IslamHany/Documents/-$ cp -r ~/IslamHany/Documents/ ~/IslamHany/Mydocuments 
osc@pop-os:~/IslamHany/Documents/-$ cd ..
osc@pop-os:~/IslamHany/Documents$ cd ..
osc@pop-os:~/IslamHany$ ls 
Documents  Mydocuments
osc@pop-os:~/IslamHany$ cd Mydocuments/
osc@pop-os:~/IslamHany/Mydocuments$ ls
-  file1.txt  file2.txt  file3.txt
osc@pop-os:~/IslamHany/Mydocuments$ cd ..
osc@pop-os:~/IslamHany$ cat .hidden_file 
osc@pop-os:~/IslamHany$ echo hiddenfilecontent >.hidden_file 
osc@pop-os:~/IslamHany$ cat .hidden_file 
hiddenfilecontent
osc@pop-os:~/IslamHany$ mv ~/IslamHany/Documents/file2.txt ~/IslamHany/
osc@pop-os:~/IslamHany$ mv ~/IslamHany/Documents/file3.txt ~/IslamHany/
osc@pop-os:~/IslamHany$ ls
Documents  file2.txt  file3.txt  Mydocuments
osc@pop-os:~/IslamHany$ cd ~
osc@pop-os:~$ rm -r IslamHany/
osc@pop-os:~$ ls 
Desktop  Documents  Downloads  Music  Pictures  Public  Task_hussein  Templates  Videos
osc@pop-os:~$ 


